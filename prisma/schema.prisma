generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id Int  @id @default(autoincrement())
  title String
  description String
  tags String[]
  urlDisplayText String?
  url String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Technical {
  id Int @id @default(autoincrement())
  imgSrc String?
  skill String
  proficiency Int
}